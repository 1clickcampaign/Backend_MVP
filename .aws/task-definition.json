{
  "family": "fastapi-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "fastapi-backend",
      "image": "058264339376.dkr.ecr.us-east-2.amazonaws.com/datapull/fastapi-backend:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SUPABASE_URL",
          "value": "{{SUPABASE_URL}}"
        },
        {
          "name": "SUPABASE_KEY",
          "value": "{{SUPABASE_KEY}}"
        },
        {
          "name": "GOOGLE_MAPS_API_KEY",
          "value": "{{GOOGLE_MAPS_API_KEY}}"
        },
        {
          "name": "API_KEY",
          "value": "{{API_KEY}}"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fastapi-backend",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::058264339376:role/ecsTaskExecutionRole"
}